{
    "name": "keplog/php",
    "description": "Keplog SDK for PHP and Laravel - Error tracking and monitoring",
    "type": "library",
    "keywords": ["keplog", "error-tracking", "monitoring", "laravel", "php", "logging"],
    "license": "MIT",
    "authors": [
        {
            "name": "Keplog",
            "email": "support@keplog.io"
        }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "mockery/mockery": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "Keplog\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Keplog\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test:coverage": "phpunit --coverage-html coverage"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
